Arrow=%ResourceDir%\Themes\gruvbox\cursors\light\arrow.cur
Crosshair=%ResourceDir%\Themes\gruvbox\light\dark\cross.cur
Hand=%ResourceDir%\Themes\gruvbox\cursors\light\link.cur
Help=%ResourceDir%\Themes\gruvbox\cursors\light\helpsel.cur
IBeam=%ResourceDir%\Themes\gruvbox\cursors\light\ibeam.cur
No=%ResourceDir%\Themes\gruvbox\cursors\light\unavail.cur
NWPen=%ResourceDir%\Themes\gruvbox\cursors\light\pen.cur
SizeAll=%ResourceDir%\Themes\gruvbox\cursors\light\move.cur
SizeNESW=%ResourceDir%\Themes\gruvbox\cursors\light\nesw.cur
SizeNS=%ResourceDir%\Themes\gruvbox\cursors\light\ns.cur
SizeNWSE=%ResourceDir%\Themes\gruvbox\cursors\light\nwse.cur
SizeWE=%ResourceDir%\Themes\gruvbox\cursors\light\ew.cur
UpArrow=%ResourceDir%\Themes\gruvbox\cursors\light\up.cur
Wait=%ResourceDir%\Themes\gruvbox\cursors\light\wait.ani
DefaultValue=Windows Default
DefaultValue.MUI=@main.cpl,-1020

[Control Panel\Desktop]
